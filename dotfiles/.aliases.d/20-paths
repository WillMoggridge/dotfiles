# Dev paths
dev=$HOME/dev
src=$dev/src

# Dev path commands
_dev() { _files -W $dev; }
dev() { cd $dev/$1; }
# type compdef &>/dev/null && compdef _dev dev

_src() { _files -W $src; }
src() { cd $src/$1; }
# type compdef &>/dev/null && compdef _src src

# Misc paths
dotfiles=$HOME/.dotfiles
dotfiles() { cd $dotfiles; }
